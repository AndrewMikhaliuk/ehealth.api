  "type": "object",
  "properties": {
    "to": {
      "type": "string",
      "description": "Coma separated receivers emails"
    },
    "subject": {
      "type": "string",
      "description": "Subject of email"
    },
    "data": {
      "type": "object"
    },
    "from": {
      "type": "string",
